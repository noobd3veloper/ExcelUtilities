<Window x:Class="ExcelUtilities.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ExcelUtilities"
        mc:Ignorable="d"
        Title="MainMenu" Height="500" Width="273"
        ResizeMode="NoResize" WindowStartupLocation="CenterScreen">
    <Grid Margin="0,0,-8,0">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="0*"/>
            <ColumnDefinition Width="0*"/>
            <ColumnDefinition Width="0*"/>
            <ColumnDefinition/>
            <ColumnDefinition Width="0*"/>
        </Grid.ColumnDefinitions>
        <Button x:Name="btnMatchF741" Grid.ColumnSpan="4" Content="Match F741" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="242" Height="28" Click="btnMatchF741_Click"/>
        <Button x:Name="btnExceltoText" Grid.ColumnSpan="4" Content="Excel to Text" HorizontalAlignment="Left" Margin="10,43,0,0" VerticalAlignment="Top" Width="242" Height="28" Click="btnExceltoText_Click"/>
    </Grid>
</Window>
